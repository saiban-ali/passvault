{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/server/src",
  "projectType": "application",
  "targets": {
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/server",
        "command": "sls deploy --verbose"
      }
    },
    "deploy:function": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/server",
        "command": "sls deploy function -f {args.name} --verbose"
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/server",
        "command": "ts-node serverless.ts"
      }
    }
  },
  "tags": []
}
